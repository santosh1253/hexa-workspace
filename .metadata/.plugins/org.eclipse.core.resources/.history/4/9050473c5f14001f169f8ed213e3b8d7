package mypackage;
import java.io.*;
import java.util.Scanner;

public class First {
	
	int a;
	float b;
	
	

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextFloat();
		
		
		System.out.println("boolean value"+sc.b);
		System.out.println("int value"+sc.a);

		
	

	}

}
