package mypackage;

public class First {
	boolean f;
	int a;
	float b;
	char ch;
	

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		
		System.out.println("boolean value"+sc.f);
		System.out.println("int value"+sc.a);

		System.out.println("float value"+sc.b);
		System.out.println("char value"+sc.ch);
	

	}

}
